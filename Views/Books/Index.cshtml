@model IEnumerable<library.Models.Book>
@{
    ViewBag.Title = "Books";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Books</h2>

<table>
    <thead>
        <tr>
            <th>
                Books
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var book in Model)
        {
            <tr>
                <td>
                    @book.Name
                </td>
            </tr>
        }
    </tbody>
</table>

